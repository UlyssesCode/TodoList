{"ast":null,"code":"import TodoHeader from \"./components/TodoHeader.vue\";\nimport TodoTasks from \"./components/TodoTasks.vue\";\nimport AddTask from \"./components/AddTask.vue\";\nexport default {\n  name: \"App\",\n  components: {\n    TodoHeader,\n    TodoTasks,\n    AddTask\n  },\n\n  data() {\n    return {\n      Tasks: [],\n      showAdd: true\n    };\n  },\n\n  methods: {\n    toggleAddTask() {\n      console.log(this.showAdd);\n      this.showAdd = !this.showAdd;\n    },\n\n    addTask(task) {\n      this.Tasks.push(task);\n    },\n\n    deleteTask(id) {\n      if (confirm(\"Sure?\")) this.Tasks = this.Tasks.filter(Task => Task.id != id);\n    },\n\n    toggleReminder(id) {\n      this.Tasks.map(Task => Task.id === id ? Task.reminder = !Task.reminder : \"\");\n    }\n\n  },\n\n  created() {\n    this.Tasks = [{\n      id: 100,\n      text: \"Study\",\n      day: \"2022.1.1\",\n      reminder: true\n    }, {\n      id: 200,\n      text: \"Eat\",\n      day: \"2022.1.1\",\n      reminder: true\n    }, {\n      id: 300,\n      text: \"Sleep\",\n      day: \"2022.1.1\",\n      reminder: false\n    }];\n  }\n\n};","map":{"version":3,"mappings":"AAeA;AACA;AACA;AAEA;EACAA,WADA;EAEAC;IACAC,UADA;IAEAC,SAFA;IAGAC;EAHA,CAFA;;EAOAC;IACA;MACAC,SADA;MAEAC;IAFA;EAIA,CAZA;;EAaAC;IACAC;MACAC;MACA;IACA,CAJA;;IAKAC;MACA;IACA,CAPA;;IAQAC;MACA,sBACA;IACA,CAXA;;IAYAC;MACA,uBACAC,oDADA;IAGA;;EAhBA,CAbA;;EA+BAC;IACA,cACA;MAAAC;MAAAC;MAAAC;MAAAC;IAAA,CADA,EAEA;MAAAH;MAAAC;MAAAC;MAAAC;IAAA,CAFA,EAGA;MAAAH;MAAAC;MAAAC;MAAAC;IAAA,CAHA;EAKA;;AArCA","names":["name","components","TodoHeader","TodoTasks","AddTask","data","Tasks","showAdd","methods","toggleAddTask","console","addTask","deleteTask","toggleReminder","Task","created","id","text","day","reminder"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <TodoHeader title=\"Todo List\" />\n    <div v-show=\"showAdd\" @click=\"toggleAddTask\">\n      <AddTask @add-task=\"addTask\" />\n    </div>\n    <TodoTasks\n      @toggle-reminder=\"toggleReminder\"\n      @delete-Task=\"deleteTask\"\n      :Tasks=\"Tasks\"\n    />\n  </div>\n</template>\n\n<script>\nimport TodoHeader from \"./components/TodoHeader.vue\";\nimport TodoTasks from \"./components/TodoTasks.vue\";\nimport AddTask from \"./components/AddTask.vue\";\n\nexport default {\n  name: \"App\",\n  components: {\n    TodoHeader,\n    TodoTasks,\n    AddTask,\n  },\n  data() {\n    return {\n      Tasks: [],\n      showAdd: true,\n    };\n  },\n  methods: {\n    toggleAddTask() {\n      console.log(this.showAdd);\n      this.showAdd = !this.showAdd;\n    },\n    addTask(task) {\n      this.Tasks.push(task);\n    },\n    deleteTask(id) {\n      if (confirm(\"Sure?\"))\n        this.Tasks = this.Tasks.filter((Task) => Task.id != id);\n    },\n    toggleReminder(id) {\n      this.Tasks.map((Task) =>\n        Task.id === id ? (Task.reminder = !Task.reminder) : \"\"\n      );\n    },\n  },\n  created() {\n    this.Tasks = [\n      { id: 100, text: \"Study\", day: \"2022.1.1\", reminder: true },\n      { id: 200, text: \"Eat\", day: \"2022.1.1\", reminder: true },\n      { id: 300, text: \"Sleep\", day: \"2022.1.1\", reminder: false },\n    ];\n  },\n};\n</script>\n\n<style>\n@import url(\"https://fonts.googleapis.com/css2?family=Poppins:wght@300;400&display=swap\");\n* {\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n}\nbody {\n  font-family: \"Poppins\", sans-serif;\n}\n.container {\n  max-width: 500px;\n  margin: 30px auto;\n  overflow: auto;\n  min-height: 300px;\n  border: 1px solid steelblue;\n  padding: 30px;\n  border-radius: 5px;\n}\n.btn {\n  display: inline-block;\n  background: #000;\n  color: #fff;\n  border: none;\n  padding: 10px 20px;\n  margin: 5px;\n  border-radius: 5px;\n  cursor: pointer;\n  text-decoration: none;\n  font-size: 15px;\n  font-family: inherit;\n}\n.btn:focus {\n  outline: none;\n}\n.btn:active {\n  transform: scale(0.98);\n}\n.btn-block {\n  display: block;\n  width: 100%;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}